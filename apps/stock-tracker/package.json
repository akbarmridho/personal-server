{
  "name": "@personal-server/stock-tracker",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "kottster dev",
    "dev:add-data-source": "kottster add-data-source",
    "build": "turbo run compile --filter={./}",
    "compile": "vite build && kottster build:server",
    "start": "node dist/server/server.cjs",
    "migrate": "pnpx dbmate -d \"./migrations\" --env-file \".env\" --no-dump-schema migrate"
  },
  "dependencies": {
    "@kottster/cli": "3.4.5",
    "@kottster/common": "3.4.5",
    "@kottster/react": "3.4.5",
    "@kottster/server": "3.4.5",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "react": "^19.x",
    "react-dom": "^19.x",
    "react-router-dom": "^7.x"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^22.16.4",
    "@types/react": "^19.x",
    "@types/react-dom": "^19.x",
    "@vitejs/plugin-react": "^4.x",
    "dbmate": "^2.28.0",
    "tsx": "^4.x",
    "typescript": "catalog:core",
    "vite": "^6.x",
    "vite-tsconfig-paths": "^4.x"
  },
  "engines": {
    "node": ">=24"
  }
}