{
  "name": "@personal-server/ai-backend",
  "version": "1.0.0",
  "description": "New iteration of AI backend with Inngest and AI SDK",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "ts-run": "node --import @swc-node/register/esm-register",
    "dev": "pnpm run cleanup && concurrently --kill-others --kill-others-on-fail \"pnpm run dev:server\" \"pnpm run dev:inngest\"",
    "cleanup": "lsof -ti:3010 | xargs kill -9 2>/dev/null || true",
    "dev:server": "pnpm ts-run src/index.ts",
    "dev:inngest": "pnpm exec inngest start --event-key 7365637265746576616e742d6576656e742d6b6579 --signing-key 7365637265747369676e696e672d6b65792d686578",
    "run": "pm2 start ecosystem.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "@ai-sdk/openai": "3.0.0-beta.72",
    "@ai-sdk/react": "3.0.0-beta.123",
    "@dotenvx/dotenvx": "^1.49.0",
    "@fastify/cors": "^11.1.0",
    "@openrouter/ai-sdk-provider": "1.0.0-beta.7",
    "@t3-oss/env-core": "^0.13.8",
    "ai": "6.0.0-beta.123",
    "fastify": "^5.6.2",
    "fastify-graceful-shutdown": "^5.0.0",
    "inngest": "^3.46.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@swc-node/register": "^1.10.10",
    "@types/node": "^22.16.4",
    "concurrently": "^9.1.0",
    "inngest-cli": "^1.14.0",
    "typescript": "^5.9.2"
  }
}
