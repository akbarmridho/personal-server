{
    "name": "@personal-server/filen",
    "version": "1.0.0",
    "description": "Filen network and webdav custom integration.",
    "main": "index.js",
    "scripts": {
        "build": "turbo run compile --filter={./}",
        "compile": "tsc",
        "format": "biome format --write src",
        "lint": "biome lint src",
        "check": "biome check src",
        "check:apply": "biome check --apply src",
        "ts-run": "turbo run compile --filter=... --filter=!{./} && node --import @swc-node/register/esm-register",
        "run": "pm2 start ecosystem.config.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "engines": {
        "node": ">=24"
    },
    "dependencies": {
        "@filen/network-drive": "^0.9.44",
        "@filen/sdk": "^0.3.11",
        "@filen/sync": "^0.2.1",
        "@filen/webdav": "^0.3.1",
        "@personal-server/common": "workspace:*",
        "@t3-oss/env-core": "catalog:core",
        "totp-generator": "^1.0.0",
        "zod": "^4.1.1"
    },
    "devDependencies": {
        "@types/node": "^22.16.4",
        "typescript": "catalog:core"
    }
}