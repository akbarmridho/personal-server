AI_CLIENT_CONNECTOR_HOST=0.0.0.0
AI_CLIENT_CONNECTOR_PORT=8787
LOG_LEVEL=info
KB_BACKEND_URL=https://kb.akbarmr.dev
AI_CLIENT_CONNECTOR_PUBLIC_PROXY_URL=http://100.x.y.z:8787

# Required for golden-article task
# GOLDEN_ARTICLE_URL=https://your-golden-article-site.example
# INNGEST_URL=https://your-inngest-endpoint.example

# Required for WhatsApp channel ingestion (set one of JID or invite)
# WHATSAPP_CHANNEL_JID=120363xxxxx@newsletter
# WHATSAPP_CHANNEL_INVITE_CODE=https://whatsapp.com/channel/0029Va...

# Optional
# Interval and timeout are constants in code.
# CDP endpoint used by Playwright to attach.
PLAYWRIGHT_CDP_URL=http://127.0.0.1:9222

# WhatsApp auth session storage directory
# WHATSAPP_AUTH_STATE_DIR=/absolute/path/to/ai-client-connector/.whatsapp-auth

# Browser restart on app startup (used to launch Brave with remote debugging).
PLAYWRIGHT_BROWSER_PATH=
PLAYWRIGHT_USER_DATA_DIR=
PLAYWRIGHT_PROFILE_DIR=
