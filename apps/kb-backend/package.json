{
    "name": "@personal-server/kb-backend",
    "version": "1.0.0",
    "description": "Backend for managing document knowledge base.",
    "main": "index.js",
    "scripts": {
        "build": "turbo run compile --filter={./}",
        "start": "node dist/index.js",
        "compile": "tsc",
        "ts-run": "turbo run compile --filter=... --filter=!{./} && node --import @swc-node/register/esm-register",
        "run": "pm2 start ecosystem.config.js",
        "migrate": "pnpx dbmate -d \"./src/db/migrations\" --env-file \".env\" --no-dump-schema migrate",
        "kysely:gen": "kysely-codegen --out-file ./src/db/types.ts",
        "playwright": "npx playwright install --with-deps",
        "camoufox": "npx camoufox-js fetch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "engines": {
        "node": ">=24"
    },
    "dependencies": {
        "@ai-sdk/openai": "catalog:core",
        "@bogeychan/elysia-logger": "^0.1.10",
        "@dotenvx/dotenvx": "catalog:core",
        "@elysiajs/cors": "^1.4.0",
        "@elysiajs/node": "^1.4.0",
        "@elysiajs/swagger": "1.2.2",
        "@openrouter/ai-sdk-provider": "catalog:core",
        "@personal-server/common": "workspace:*",
        "@t3-oss/env-core": "catalog:core",
        "ai": "catalog:core",
        "ai-fallback": "catalog:core",
        "cheerio": "^1.1.2",
        "dayjs": "^1.11.18",
        "camoufox-js": "^0.7.0",
        "dom-to-semantic-markdown": "^1.5.0",
        "elysia": "^1.4.7",
        "fastmcp": "^3.17.0",
        "graceful-server-elysia": "^1.0.25",
        "he": "^1.2.0",
        "jsdom": "^27.0.0",
        "kysely": "^0.28.7",
        "mdast-util-from-markdown": "^2.0.2",
        "mdast-util-gfm": "^3.1.0",
        "mdast-util-to-markdown": "^2.1.2",
        "mdast-util-to-string": "^4.0.0",
        "micromark-extension-gfm": "^3.0.0",
        "p-retry": "^7.0.0",
        "pg": "^8.16.3",
        "playwright": "^1.55.1",
        "prettier": "^3.6.2",
        "text-segmentation": "^1.0.3",
        "voyageai": "^0.0.8",
        "zod": "catalog:core"
    },
    "devDependencies": {
        "@types/he": "^1.2.3",
        "@types/jsdom": "^27.0.0",
        "@types/mdast": "^4.0.4",
        "@types/node": "^22.16.4",
        "@types/pg": "^8.15.5",
        "dbmate": "^2.28.0",
        "kysely-codegen": "^0.19.0",
        "typescript": "catalog:core"
    }
}